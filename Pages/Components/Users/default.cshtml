@model List<ViewComponentSys.Domain.User>

<h1>List of Users</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">User Name</th>
            <th scope="col">Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
        <tr>
      
      
            <td>@user.Name</td>
            <td>@user.Username</td>
            <td>@user.Email</td>
            <td><a asp-page="/details" asp-route-id="@user.Id">Details</a></td>
        </tr>
        }
    </tbody>
</table>